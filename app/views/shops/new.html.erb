<div class="main shops-new">
  <div class="container">
    <h1 class="form-heading">登録する</h1>
    <div class="form">
      <div class="form-body">
        <%= form_tag("/shops/create", {multipart: true}) do %>
            <p>ショップ名 [必須]</p>
            <input name="name" placeholder="D2Correct">
            <p>ショップURL [必須]</p>
            <input name="url" placeholder="https://www.d2correct.com/">
            <p>アイコン</p>
            <input name="icon" type="file">
            <input type="submit" value="登録">
        <% end %>
      </div>
    </div>
  </div>
</div>