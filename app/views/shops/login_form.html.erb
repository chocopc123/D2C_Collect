<div class="main shops-new">
  <div class="container">
    <div class="form-heading">ログイン</div>
    <div class="form shops-form">
      <div class="form-body">
        <%= form_tag("/shops/login") do %>
          <div class="form-error">
            <%= @error_message %>
          </div>
          <p>メールアドレス</p>
          <input name="email" value=<%= @email %>>
          <p>パスワード</p>
          <input type="password" name="password" value=<%= @password %>>
          <input type="submit" value="ログイン">
        <% end %>
      </div>
    </div>
  </div>
</div>