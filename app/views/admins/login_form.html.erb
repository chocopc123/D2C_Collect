<div class="main admins-login_form">
  <div class="container">
    <div class="form-heading">Admin Login</div>
    <div class="form admins-form">
      <div class="form-body">
        <%= form_tag("/admins/login") do %>
          <div class="form-error">
            <%= @error_message %>
          </div>
          <p>ID</p>
          <input name="admin_id" value="<%= @email %>">
          <p>Password</p>
          <input type="password" name="password" value="<%= @password %>">
          <input type="submit" value="Login">
        <% end %>
      </div>
    </div>
  </div>
</div>